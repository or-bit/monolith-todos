{
  "extends": "airbnb",
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "linebreak-style": 0,
    "max-len": [
      "error",
      {
        "ignoreComments": true
      }
    ],
    "react/forbid-prop-types": "off",
    "no-unused-expressions": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}